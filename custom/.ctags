--langdef=MYLUA
--langmap=MYLUA:.lua
--regex-MYLUA=/^.*\s*function[ \t]*([a-zA-Z0-9_]+):([a-zA-Z0-9_]+).*$/\2/f/
--regex-MYLUA=/^.*\s*function[ \t]*([a-zA-Z0-9_]+)\.([a-zA-Z0-9_]+).*$/\2/f/
--regex-MYLUA=/^.*\s*function[ \t]*([a-zA-Z0-9_]+)\s*\(.*$/\1/f/
--regex-MYLUA=/^[ \t]*([a-zA-Z0-9_]+)[ \t]*=.*$/\1/e/
--regex-MYLUA=/^[ \t]*module[ \t\(]*\"([a-zA-Z0-9_]+)\".*$/\1/m,module/
